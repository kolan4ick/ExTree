<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.extree">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/extree_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/extree_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExTree">
        <activity
            android:name=".FirstLaunchActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Extree.FirstLaunch" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>